Patient Table:
CREATE TABLE checkthat_patient_tbl(Patient_ID int auto_increment, User_ID int, Patient_Last_Name varchar(30), Patient_First_Name varchar(30), Patient_Mobile_Number bigint, Patient_Age int, Patient_Gender varchar(20), primary key(Patient_ID));



Service Info Table:
CREATE TABLE checkthat_serviceinfo_tbl(ServiceID int auto_increment, User_ID int, Patient_ID int, Package_ID int, Profile_ID int, Test_ID int, SampleCollection_flag boolean, Inperson_flag boolean, isTransport boolean, Address1 varchar(255), Address2 varchar(255), City varchar(30), State varchar(30), Zipcode varchar(30), Appointment_date varchar(30), Total_Cost varchar(30), primary key(ServiceID));


Sales Info Table
CREATE TABLE checkthat_salesinfo_tbl(SalesID int auto_increment, Service_ID int, User_ID int, Patient_ID int, Package_ID int, Profile_ID int, Test_ID int, Total_Cost varchar(30), Transcation_ID bigint, Transcation_Status varchar(30), primary key(SalesID));
